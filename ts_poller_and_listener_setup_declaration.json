{
    "class": "Telemetry",
    "controls": {
        "class": "Controls",
        "logLevel": "debug"
    },
    "My_Poller": {
        "class": "Telemetry_System_Poller",
        "interval": 60
    },
    "My_Consumer": {
        "class": "Telemetry_Consumer",
        "type": "Azure_Log_Analytics",
        "workspaceId": "<<workspace-id>>",
        "passphrase": {
            "cipherText": "<<sharedkey>>"
        },
        "useManagedIdentity": false,
        "region": "eastus"
    }
}
